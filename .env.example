# Configuração da API de IA para geração de relatórios DISC

# OpenAI API (recomendado)
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_AI_API_URL=https://api.openai.com/v1

# Configurações alternativas (opcional)
# VITE_AI_API_URL=https://api.anthropic.com/v1  # Para Claude
# VITE_AI_API_URL=http://localhost:11434/v1    # Para Ollama local

# Configurações do relatório
VITE_AI_REPORT_MODEL=gpt-3.5-turbo
VITE_AI_REPORT_TEMPERATURE=0.7
VITE_AI_REPORT_MAX_TOKENS=2000